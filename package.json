{
    "name": "flowlint",
    "version": "0.7.6",
    "description": "CLI for static analysis of n8n workflows",
    "bin": {
        "flowlint": "./dist/cli.js"
    },
    "main": "dist/cli.js",
    "files": [
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsx src/cli.ts",
        "test": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "n8n",
        "workflow",
        "linter",
        "cli",
        "static-analysis"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Replikanti/flowlint-cli.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=24.12.0"
    },
    "dependencies": {
        "commander": "^14.0.2",
        "glob": "^13.0.0",
        "picocolors": "^1.1.1",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "@octokit/rest": "22.0.1",
        "@replikanti/flowlint-core": "0.9.1",
        "@types/node": "24.10.4",
        "openai": "6.15.0",
        "tsup": "8.5.1",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    }
}
